{
  "name": "artpulse-management-plugin",
  "version": "0.1.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:sass && npm run build:css && wp-scripts build && npm run bundle:react && npm run build:babel",
    "build:dev": "NODE_ENV=development npm run build",
    "build:sass": "sass assets/scss:assets/css --no-source-map",
    "build:css": "postcss assets/css/*.css -d assets/css/min/",
    "build:react": "vite build",
    "build:babel": "npm run babel:sidebar && npm run babel:taxonomy && npm run babel:filtered-list && npm run babel:ajax-filter",
    "babel:sidebar": "npx babel assets/js/sidebar-taxonomies.jsx -o assets/js/sidebar-taxonomies.js",
    "babel:taxonomy": "npx babel assets/js/advanced-taxonomy-filter-block.jsx -o assets/js/advanced-taxonomy-filter-block.js",
    "babel:filtered-list": "npx babel assets/js/filtered-list-shortcode-block.jsx -o assets/js/filtered-list-shortcode-block.js",
    "babel:ajax-filter": "npx babel assets/js/ajax-filter-block.jsx -o assets/js/ajax-filter-block.js",
    "bundle:react": "rollup -c",
    "dev": "vite",
    "start": "wp-scripts start",
    "release": "standard-version",
    "test": "jest --config=jest.config.cjs",
    "test:js": "jest --config=jest.config.cjs",
    "test:e2e": "node tests/e2e/monetization-workflow.js",
    "test:php": "WP_PHPUNIT__TESTS_CONFIG=wp-tests-config.php ./vendor/bin/phpunit",
    "test:ui": "npm run cypress:run",
    "test:all": "npm run test:php && npm run test:ui",
    "cypress:run": "node scripts/run-cypress.js",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "widget:audit": "node scripts/widget-audit.js",
    "widget:fix": "node scripts/fix-widget-callbacks.js",
    "ws": "node server/ws-server.js"
  },
  "dependencies": {
    "emoji-picker-react": "^4.13.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.346.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.20",
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.23.3",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@wordpress/scripts": "^19.2.4",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.4",
    "cssnano": "^6.0.1",
    "cypress": "^14.5.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "glob": "^11.0.3",
    "jest": "^30.0.4",
    "jsdom": "^21.0.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "puppeteer": "^21.5.3",
    "purgecss": "^4.1.3",
    "rollup": "^4.44.2",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.69.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.4.5",
    "vite": "^5.0.11"
  }
}
