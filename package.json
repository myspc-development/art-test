{
  "name": "artpulse-management-plugin",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "build:css": "sass assets/scss:assets/css --no-source-map",
    "build": "npm run build:css && npx tailwindcss -i assets/css/tailwind.css -o public/tailwind.build.css && wp-scripts build && npx babel assets/js/OrganizationSubmissionForm.jsx -o assets/js/ap-org-submission.js",
    "start": "wp-scripts start",
    "release": "standard-version",
    "watch:tailwind": "npx tailwindcss -i assets/css/tailwind.css -o public/tailwind.build.css --watch"
  },
  "devDependencies": {
    "@wordpress/scripts": "^19.2.4",
    "standard-version": "^9.5.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.4",
    "postcss": "^8.4.38",
    "autoprefixer": "^10.4.17"
  }
}
