<?xml version="1.0" encoding="UTF-8"?>
<!-- Template for PHPUnit unit tests; copy to phpunit.unit.xml for local overrides. -->
<phpunit bootstrap="tests/bootstrap.php" colors="true">
  <testsuites>
    <testsuite name="Unit">
      <directory suffix="Test.php">tests/Unit</directory>
    </testsuite>
  </testsuites>
</phpunit>
