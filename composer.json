{
  "name": "artpulse/art-test",
  "autoload": {
    "psr-4": {
      "ArtPulse\\": "src/",
      "ArtPulse\\Widgets\\": "widgets/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArtPulse\\Tests\\Stubs\\": "tests/Stubs/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "yoast/phpunit-polyfills": "^2.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "phpcompatibility/phpcompatibility-wp": "*",
    "wp-phpunit/wp-phpunit": "^6",
      "10up/wp_mock": "^0.4"
  },
  "require": {
    "yahnis-elsts/plugin-update-checker": "^5.6",
    "erusev/parsedown": "^1.7"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "sniff": "phpcs --standard=phpcs.xml",
    "zip": "bash scripts/release.sh",
    "widgets:list": "php bin/list-widgets"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
