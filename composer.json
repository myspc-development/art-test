{
  "name": "artpulse/art-test",
  "autoload": {
    "psr-4": {
      "ArtPulse\\": "src/",
      "ArtPulse\\Widgets\\": "widgets/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArtPulse\\Tests\\Stubs\\": "tests/Stubs/"
    }
  },
  "require-dev": {
    "wp-phpunit/wp-phpunit": "^6",
    "yoast/phpunit-polyfills": "^3.1",
    "brain/monkey": "^2.6",
    "phpunit/phpunit": "^9.6",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "squizlabs/php_codesniffer": "^3.8",
    "wp-coding-standards/wpcs": "^3.0",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "require": {
    "yahnis-elsts/plugin-update-checker": "^5.6",
    "erusev/parsedown": "^1.7"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:debug": "XDEBUG_MODE=coverage vendor/bin/phpunit -d xdebug.mode=debug,coverage",
    "cs": "vendor/bin/phpcs",
    "cbf": "vendor/bin/phpcbf"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
